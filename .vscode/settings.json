{
  // VSCode
  "explorer.compactFolders": false,
  "editor.rulers": [120, 160],
  // "files.exclude": {
  //   "**/*.pyc": {
  //     "when": "$(basename).py"
  //   },
  //   "**/__pycache__": true,
  //   "**/*.pytest_cache": true,
  //   "**/Sikuli_java**": true
  // },
  "files.associations": {
    "*.robocop": "toml",
    "*.robotidy": "toml"
  },
  // "terminal.integrated.env.windows": {
  //   "PYTHONPATH": "${workspaceFolder}"
  // },
  "workbench.colorCustomizations": {
    "editorInfo.foreground": "#006eff", 
    "editorError.foreground": "#F00",
    "editorWarning.foreground": "#ffa23e"
  },

  // Code Runner
  "code-runner.clearPreviousOutput": true,
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "python": "cls ; python -u",
    "powershell": "cls ; powershell -ExecutionPolicy ByPass -File"
  },
  "code-runner.executorMapByFileExtension": {
    ".robot": "cls ; robot --argumentfile .\\config\\argumentos_cli.cfg"
  },

  // Python
  "python.defaultInterpreterPath": "${workspaceFolder}\\.vscode\\.venv\\Scripts\\python.exe",
  "python.experiments.optInto": ["pythonTerminalEnvVarActivation"],

  // Black Formatter Python
  "black-formatter.args": ["--line-length=160"],

  // RobotCode
  // "robotcode.robot.languages": [".\\config\\PTBR_InovaRobot.py"],
  // "robotcode.robot.variableFiles": [".\\config\\variaveis_globais.py"],
  "robotcode.testExplorer.enabled": false,
  // "robotcode.robot.pythonPath": ["."],
  "robotcode.analysis.diagnosticMode": "workspace",
  "robotcode.analysis.progressMode": "detailed",
